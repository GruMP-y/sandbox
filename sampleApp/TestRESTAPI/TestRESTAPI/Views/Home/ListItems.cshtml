
@model IEnumerable<TestRESTAPI.Models.MyItem>
@{
    ViewBag.Title = "ListItems";
}

<h2>List of Items</h2>

<table border="1">
   <tr>
     <th width="100">ID</th>
    <th width="500">Value</th>
   </tr>
    @foreach (var itm in Model) { 
        <tr>
            <td>@itm.ID</td>
            <td>@itm.Value</td>
        </tr> 
    }
</table>

